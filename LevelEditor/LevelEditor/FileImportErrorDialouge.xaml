<Window x:Class="LevelEditor.FileImportErrorDialouge"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LevelEditor"
        mc:Ignorable="d"
        Title="File Import Error" Height="150" Width="250">
    <DockPanel>
        <TextBlock Margin="0,15,0,0" HorizontalAlignment="Center" DockPanel.Dock="Top" TextAlignment="Center">
            Unable to Import Level or metadata  <LineBreak/>
            file. Check that you are trying to<LineBreak/>
            load a valid level and the metadata <LineBreak/>
            file exists.
        </TextBlock>
        <Button Margin="0,5,0,0" Content="Ok" DockPanel.Dock="Bottom"  Click="OkButtonClick"/>
    </DockPanel>
</Window>
